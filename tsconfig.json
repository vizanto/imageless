{
  "compilerOptions": {
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "rootDir": "src",
    "outDir": "lib"
  },
  "exclude": [
    "node_modules"
  ],
  "plugins": [
    {
      "name": "@divyenduz/ts-graphql-plugin"
    }
  ]
}
